{
  "build": {
    "cleanupCacheHistory": false,
    "content": [
      {
        "files": [ "api/**.yml", "api/index.md" ]
      },
      {
        "files": [ "articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md" ]
      }
    ],
    "dest": "_site",
    "disableGitFeatures": false,
    "fileMetadataFiles": [],
    "globalMetadataFiles": [],
    "keepFileLink": false,
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "overwrite": [
      {
        "files": [ "apidoc/**.md" ],
        "exclude": [ "obj/**", "_site/**" ]
      }
    ],
    "postProcessors": [],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "template": [ "default", "templates/material", "templates/ubiety" ]
  },
  "metadata": [
    {
      "dest": "api",
      "disableDefaultFilter": false,
      "disableGitFeatures": false,
      "src": [
        {
          "files": [ "**.csproj" ],
          "src": "../../src/"
        }
      ]
    }
  ]
}
